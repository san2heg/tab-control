{
  "manifest_version": 2,
  "name": "Tab Control",
  "version": "1.0.0",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "TabWrapper.js",
      "eventPage.js"
    ],
    "persistent": false
  },

  "description": "A chrome extension to manage tabs.",

  "browser_action": {
    "default_title": "Tab Control",
    "default_popup": "popup.html"
  },

  "author": "Sanketh Hegde"
}
