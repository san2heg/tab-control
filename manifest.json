{
  "manifest_version": 2,
  "name": "Tab Control",
  "version": "1.0.0",
  "permissions": [
    "tabs"
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },

  "description": "A chrome extension to manage tabs.",

  "browser_action": {
    "default_title": "Tab Control",
  },

  "author": "Sanketh Hegde"
}
