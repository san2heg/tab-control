{
  "manifest_version": 2,
  "name": "Tab Control",
  "version": "1.0.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/TabWrapper.js",
      "js/background.js"
    ]
  },

  "description": "A chrome extension to control your tabs.",

  "browser_action": {
    "default_title": "Tab Control",
    "default_popup": "popup.html"
  },

  "author": "Sanketh Hegde"
}
