{
  "manifest_version": 2,
  "name": "Tab Control",
  "version": "1.3.3",
  "permissions": [
    "tabs",
    "storage"
  ],

  "background": {
    "scripts": [
      "js/constants.js",
      "js/TabWrapper.js",
      "js/background.js"
    ]
  },

  "description": "Tab Control stresses over your tabs so you don't have to.",

  "browser_action": {
    "default_title": "Tab Control",
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },

  "author": "Sanketh Hegde"
}
